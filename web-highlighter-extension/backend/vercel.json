{
  "version": 2,
  "functions": {
    "api/highlights.js": {
      "runtime": "nodejs18.x"
    },
    "api/test.js": {
      "runtime": "nodejs18.x"
    }
  },
  "routes": [
    {
      "src": "/api/highlights",
      "dest": "/api/highlights.js"
    },
    {
      "src": "/api/test",
      "dest": "/api/test.js"
    }
  ],
  "env": {
    "MONGO_URI": "@mongo_uri",
    "CORS_ORIGIN": "@cors_origin"
  }
}
